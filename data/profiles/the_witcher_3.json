      },
      "dxvk-shader": {
        "cpu-affinity": ["0", "1", "2", "3", "4", "5", "6", "7"],
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-cs": {
        "cpu-affinity": ["1"],
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-queue": {
        "cpu-affinity": ["0"],
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-writer": {
        "cpu-affinity": ["2"],
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-submit": {
        "cpu-affinity": ["3"],
