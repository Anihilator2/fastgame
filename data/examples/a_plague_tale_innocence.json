{
  "executable-name": "APlagueTaleInnocence_x64",
  "environment": [
    "STAGING_SHARED_MEMORY=1",
    "WINEFSYNC_SPINCOUNT=2",
    "RADV_DEBUG=zerovram",
    "RADV_PERFTEST=dccmsaa,dfsm,shader_ballot,sisched"
  ],
  "threads": {
    "names": {
      "APlagueTaleInno": {
        "niceness": "-4",
        "scheduler-policy": "SCHED_BATCH",
        "io-class": "RT",
        "io-priority": "0"
      },
      "APlagueTal:cs0": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "APlague:disk$0": {
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-shader": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-cs": {
        "cpu-affinity": ["1"],
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-queue": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-writer": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "dxvk-submit": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "SDLAudioP2": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "SDLAudioP3": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "SDLAudioP4": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "PulseHotplug": {
        "scheduler-policy": "SCHED_OTHER"
      }
    }
  }
}
