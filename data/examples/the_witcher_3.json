{
  "executable-name": "witcher3",
  "environment": ["STAGING_SHARED_MEMORY=1"],
  "threads": {
    "names": {
      "witcher3": {
        "niceness": "-4",
        "scheduler-policy": "SCHED_BATCH",
        "io-class": "RT",
        "io-priority": "0"
      },
      "witcher:disk$0": {
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-shader": {
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-cs": {
        "cpu-affinity": ["1"],
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-queue": {
        "cpu-affinity": ["0"],
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-writer": {
        "cpu-affinity": ["2"],
        "scheduler-policy": "SCHED_IDLE"
      },
      "dxvk-submit": {
        "cpu-affinity": ["3"],
        "scheduler-policy": "SCHED_IDLE"
      },
      "SDLAudioP2": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "SDLAudioP3": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "SDLAudioP4": {
        "scheduler-policy": "SCHED_OTHER"
      },
      "PulseHotplug": {
        "scheduler-policy": "SCHED_OTHER"
      }
    }
  }
}
