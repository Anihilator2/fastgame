{
  "general": {
    "cpu": {
      "governor": {
        "default": "schedutil",
        "game": "performance"
      },
      "scheduler": {
        "sched_child_runs_first": {
          "default": "0",
          "game": "1"
        }
      }
    },
    "memory": {
