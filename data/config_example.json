{
  "general": {
    "cpu": {
      "governor": {
        "default": "schedutil",
        "game": "performance"
      },
      "scheduler": {
        "sched_child_runs_first": {
          "default": "0",
          "game": "1"
        }
      }
    },
    "disk": {
      "device": "sdc",
      "scheduler": {
        "default": "mq-deadline",
        "game": "bfq"
      },
      "read-ahead": {
        "default": "256",
        "game": "1024"
      },
      "nr-requests": {
        "default": "64",
        "game": "1024"
      },
      "rq-affinity": {
        "default": "2",
        "game": "1"
      }
    },
    "nvidia": {
      "powermizer-mode": {
        "default": "adaptive",
        "game": "maximum-performance"
      },
      "power-limit": {
        "default": "180",
        "game": "200"
      },
      "clock-offset": {
        "gpu": {
          "default": "0",
          "game": "150"
        },
        "memory": {
          "default": "0",
          "game": "500"
        }
      }
    }
  },
